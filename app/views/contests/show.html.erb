<h1>
    <%= @contest.name %>
</h1>

<table>
    <tr>
        <th>Location &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;</th>
        <th>Date</th>
    </tr>
    <tr>
        <td> <%= @contest.location %>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;</td>
        <td> <%= @contest.date %>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;</td>
        <td> <%= link_to 'Edit', edit_contest_path(@contest)%>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;</td>
        <td> <%= link_to 'Destroy', @contest, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>     
</table>
<br/>
<h3>Categories</h3>
<div>
    <% @contest.categories.each do |category| %>
        <br/>
        <%= category.name %>&nbsp;
        <%= link_to 'Show', contest_category_path(@contest, category)%>&nbsp;
        <%= link_to 'Edit', edit_contest_category_path(@contest, category)%>&nbsp;
        <%= link_to 'Destroy', [@contest, category], :confirm => 'Are you sure?', :method => :delete %></td>
    <% end %>   
</div>
<%= link_to 'New Category', new_contest_category_path(@contest,@category) %>
<br/>
<h3>Projects</h3>
<div>
    <% @contest.projects.each do |project| %>
        <br/>
        <%= project.name %>&nbsp;
        <%= link_to 'Show', contest_project_path(@contest, project)%>&nbsp;
        <%= link_to 'Edit', edit_contest_project_path(@contest, project)%>&nbsp;
        <%= link_to 'Destroy', [@contest, project], :confirm => 'Are you sure?', :method => :delete %></td>
    <% end %>   
</div>
<br/>

<%= link_to 'Add project', new_contest_project_path(@contest) %><br/>
<br/>
<%= link_to 'Upload Projects File', new_contest_attachment_path(@contest) %>
<br/>
<%= link_to 'Add Judges to projects', controller: "users" %>
<br/>
<%= link_to 'Download Scores', controller: "scores" %>
<br/>

<%= link_to 'Back to Contests', contests_path %>
<br/>
<%= link_to 'Add Question Types', question_types_path %>
<br/>
<%= link_to 'Add Questions', questions_path %>
<br/>
<%= link_to 'Judge Project Mapping', action:'show' , controller:'judges' %>
